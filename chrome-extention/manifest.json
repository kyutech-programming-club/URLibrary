{
  "name": "URLibrary",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "URL共有",
  "icons": {
    "16": "share.png",
    "48": "share.png",
    "128": "share.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "share.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background":{
    "scripts":[
      "bg.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
